{
	"name": "halbkreativ/typo3api",
	"type": "typo3-cms-extension",
	"license": "MIT",
	"keywords": [
		"TYPO3",
		"api",
		"TCA",
		"tsconfig",
		"ext_tables.php",
		"ext_localconf.php"
	],
	"support": {
		"issues": "https://github.com/halbkreativ/typo3api/issues"
	},
	"authors": [
		{
			"name": "Marco Pfeiffer",
			"role": "Developer",
			"homepage": "https://www.marco.zone/"
		},
		{
			"name": "Ingo Fabbri",
			"role": "Developer"
		}
	],
	"extra": {
		"typo3/cms": {
			"web-dir": "web",
			"extension-key": "typo3api"
		}
	},
	"require": {
		"php": "^8.0",
		"symfony/intl": "^5 || ^6",
		"symfony/options-resolver": "^5 || ^6",
		"symfony/polyfill-mbstring": "^1",
		"symfony/polyfill-php80": "^1.23.0",
		"typo3/cms-backend": "^11.5 || ^12.4",
		"typo3/cms-core": "^11.5 || ^12.4",
		"typo3/cms-extbase": "^11.5 || ^12.4",
		"typo3/cms-frontend": "^11.5 || ^12.4",
		"typo3/cms-install": "^11.5 || ^12.4",
		"typo3/cms-lang": "^11.5 || ^12.4"
	},
	"autoload": {
		"psr-4": {
			"Typo3Api\\": "Classes"
		}
	},
	"require-dev": {
		"friendsoftypo3/phpstan-typo3": "^0.9.0",
		"ssch/typo3-rector": "^1.1",
		"symplify/easy-coding-standard": "^11",
		"typo3/testing-framework": "^6.16.6"
	},
	"autoload-dev": {
		"psr-4": {
			"Typo3Api\\": "Tests"
		}
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	}
}
